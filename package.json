{
  "name": "pj2md",
  "version": "1.3.0",
  "preferGlobal": true,
  "description": "Generate a README in markdown from a package.json file",
  "main": "index.js",
  "engines": {
    "node": ">=0.12.0"
  },
  "scripts": {
    "test": "xo",
    "readme": "node bin/pj2md --no-module --no-api -o README.md -f"
  },
  "bin": {
    "pj2md": "bin/pj2md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joakimbeng/pj2md.git"
  },
  "keywords": [
    "package.json",
    "markdown",
    "package",
    "readme",
    "info"
  ],
  "author": "Joakim Carlstein <joakim@klei.se>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joakimbeng/pj2md/issues"
  },
  "homepage": "https://github.com/joakimbeng/pj2md#readme",
  "dependencies": {
    "camelcase": "^1.1.0",
    "get-api": "^1.0.1",
    "gh-repo-to-user": "^1.1.0",
    "handlebars": "^3.0.3",
    "look-up": "^0.7.1",
    "object-assign": "^4.0.1",
    "parse-author": "^0.2.0",
    "path-exists": "^2.0.0",
    "pify": "^1.1.0",
    "yargs": "^3.14.0"
  },
  "devDependencies": {
    "xo": "^0.5.2"
  },
  "xo": {
    "space": true
  }
}
