{
  "name": "pj2md",
  "version": "4.1.0",
  "preferGlobal": true,
  "description": "Generate a README in markdown from a package.json file",
  "main": "src/index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "xo && ava",
    "readme": "node bin/pj2md -o README.md -f",
    "postreadme": "cp -f README.md test/fixtures/subfolder/"
  },
  "bin": {
    "pj2md": "bin/pj2md.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joakimbeng/pj2md.git"
  },
  "keywords": [
    "package.json",
    "markdown",
    "package",
    "readme",
    "info"
  ],
  "author": "Joakim Carlstein <joakim@beng.se> (http://joakim.beng.se)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joakimbeng/pj2md/issues"
  },
  "homepage": "https://github.com/joakimbeng/pj2md#readme",
  "dependencies": {
    "arrify": "^1.0.1",
    "camelcase": "^2.1.0",
    "get-api": "^1.0.1",
    "get-pkgs": "^0.3.0",
    "gh-repo-to-user": "^1.1.0",
    "handlebars": "^4.0.5",
    "lazy-promise": "^3.0.1",
    "meow": "^3.7.0",
    "object-assign": "^4.0.1",
    "parse-author": "^0.2.0",
    "path-exists": "^2.1.0",
    "pify": "^2.3.0",
    "pkg-conf": "^1.1.1",
    "promise-all": "^1.0.0",
    "promise-and": "^1.0.0",
    "promise-fncall": "^1.1.1",
    "promise-get": "^1.0.0",
    "promise-or": "^1.0.0",
    "read-pkg-up": "^1.0.1"
  },
  "devDependencies": {
    "ava": "^0.11.0",
    "xo": "^0.12.1"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "envs": [
      "node"
    ]
  },
  "pj2md": {
    "module": false,
    "api": false
  }
}
