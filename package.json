{
  "name": "pj2md",
  "version": "1.2.0",
  "preferGlobal": true,
  "description": "Generate a README in markdown from a package.json file",
  "main": "index.js",
  "scripts": {
    "test": "xo --space",
    "readme": "node bin/pj2md --no-module --no-api -o README.md -f"
  },
  "bin": {
    "pj2md": "bin/pj2md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joakimbeng/pj2md.git"
  },
  "keywords": [
    "package.json",
    "markdown",
    "package",
    "readme",
    "info"
  ],
  "author": "Joakim Carlstein <joakim@klei.se>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joakimbeng/pj2md/issues"
  },
  "homepage": "https://github.com/joakimbeng/pj2md#readme",
  "dependencies": {
    "bluebird": "^2.9.30",
    "camelcase": "^1.1.0",
    "function-params": "^0.1.0",
    "handlebars": "^3.0.3",
    "read-package-json": "^2.0.0",
    "yargs": "^3.14.0"
  },
  "devDependencies": {
    "xo": "^0.5.2"
  }
}
